{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capability for caldav-tasks",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://**"
        },
        {
          "url": "https://**"
        }
      ]
    },
    "core:window:allow-show",
    "dialog:default",
    "fs:default",
    "shell:allow-open",
    "notification:default"
  ]
}
